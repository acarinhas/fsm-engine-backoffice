<!doctype html>
<html lang="en">
<head>
    <%- include('views/header.ejs') %>
</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-background-color="white" data-active-color="danger">

        <!--
            Tip 1: you can change the color of the sidebar's background using: data-background-color="white | black"
            Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
        -->

        <div class="sidebar-wrapper">
            <div class="logo">
                <a href="http://www.creative-tim.com" class="simple-text">
                    Creative Tim
                </a>
            </div>

            <ul class="nav">
                <li>
                    <a href="/statemachines">
                        <i class="ti-panel"></i>
                        <p>State Machines</p>
                    </a>
                </li>
                <li>
                    <a href="/versions">
                        <i class="ti-panel"></i>
                        <p>Versions</p>
                    </a>
                </li>
                <li class="active">
                    <a href="/instances">
                        <i class="ti-panel"></i>
                        <p>Instances</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar bar1"></span>
                        <span class="icon-bar bar2"></span>
                        <span class="icon-bar bar3"></span>
                    </button>
                    <a class="navbar-brand" href="#">Instances</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#">
                                <i class="ti-settings"></i>
                                <p>Settings</p>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Full list</h4>
                                <!--<p class="category">24 Hours performance</p>-->
                            </div>
                            <div class="content table-responsive table-full-width">
                                    <table class="table table table-striped">
                                        <thead>
                                        <th>ID</th>
                                        <th>versionID</th>
                                        <th>hasStarted</th>
                                        <th>hasEnded</th>
                                        <th>createdAt</th>
                                        <th>updatedAt</th>
                                        <th>action</th>
                                        </thead>
                                        <tbody>
                                        <% for(var instance of instances) { %>
                                        <tr>
                                            <td><%= instance.id %></td>
                                            <td><%= instance.versionID %></td>
                                            <td><%= instance.hasStarted %></td>
                                            <td><%= instance.hasEnded %></td>
                                            <td><%= instance.createdAt %></td>
                                            <td><%= instance.updatedAt %></td>
                                            <td>
                                                <a href="instance?id=<%= instance.id%>" class="link">view</a>
                                            </td>
                                        </tr>
                                        <% } %>
                                        </tbody>
                                    </table>
                                </div>
                                <!--<div class="footer">-->
                                <!--<hr>-->
                                <!--<div class="stats">-->
                                <!--Fine-->
                                <!--</div>-->
                                <!--</div>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <%- include('views/footer.ejs') %>
</div>
</div>
</body>


</html>

